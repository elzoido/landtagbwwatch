<h1>Alle Kategorien</h1>

<% IF kategorien.size %>
<ul>
	<% FOREACH id IN kategorien.keys %>
	<li><a href="/kategorien/<% id %>"><% kategorien.$id.name %></a></li>
	<% END %>
</ul>
<% END %>

